{
  "name": "@acpaas-ui/ngx-components",
  "description": "Antwerp City Platform as a Service User Interface (ACPaas UI) is a component interface library for building user interfaces and responsive web apps.",
  "version": "1.1.0",
  "keywords": [
    "Angular",
    "Digipolis",
    "acpaas-ui"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/digipolisantwerp/acpaas-ui_angular"
  },
  "scripts": {
    "bootstrap": "node scripts/bootstrap.js",
    "package": "node scripts/generate/package.js",
    "styleguide": "./node_modules/.bin/ng serve styleguide",
    "start": "npm run styleguide",
    "ng": "./node_modules/.bin/ng",
    "lint": "./node_modules/.bin/ng lint",
    "test": "./node_modules/.bin/ng test",
    "e2e": "./node_modules/.bin/ng e2e",
    "build": "node scripts/build.js",
    "postbuild": "node scripts/prepublish.js",
    "examples": "node scripts/examples.js",
    "example": "node scripts/generate/example.js",
    "bump": "node scripts/version.js",
    "deploy": "npm run deploy:patch",
    "deploy:major": "npm run bump -- --major && npm run build",
    "deploy:minor": "npm run bump -- --minor && npm run build",
    "deploy:patch": "npm run bump -- --patch && npm run build",
    "postdeploy:major": "npm publish dist",
    "postdeploy:minor": "npm publish dist",
    "postdeploy:patch": "npm publish dist",
    "styleguide:analyse": "ng build styleguide --stats-json && npm run styleguide:explore",
    "styleguide:explore": "webpack-bundle-analyzer dist/styleguide/stats.json"
  },
